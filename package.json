    "test:vitest:utils": "vitest run \"src/utils/**/__tests__/**/*.test.js\"",
    "test:vitest:coverage": "vitest run --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
